{
  "name": "react-simple-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "core-js": "^2.5.3",
    "csvtojson": "^1.1.9",
    "eventemitter3": "^3.0.1",
    "jsonfile": "^4.0.0",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-font-face": "^0.2.1",
    "react-loadable": "^5.3.1",
    "react-redux": "5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-spinners": "^0.2.6",
    "react-tippy": "^1.2.2",
    "read-yaml": "^1.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "css-loader": "^0.28.9",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "fetch-mock": "5.13.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react-scripts": "1.0.17",
    "react-test-renderer": "^16.1.1",
    "redux-mock-store": "^1.3.0",
    "storybook-router": "^0.3.2",
    "style-loader": "^0.20.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "copy-data": "cp ../data/json/ref.json public ",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p copy-data generate-config-dev watch-css start-js",
    "build": "npm-run-all -p generate-config-prod build-css && react-scripts build ",
    "test": "npm run build-css && react-scripts test --env=jsdom --verbose",
    "eject": "react-scripts eject",
    "lint": "eslint src/* stories/*",
    "start-sb": "start-storybook -p 9001 -c .storybook",
    "storybook": "npm-run-all -p watch-css start-sb",
    "generate-config-dev": "node config.js 'development'",
    "generate-config-prod": "node config.js 'production'"
  }
}
